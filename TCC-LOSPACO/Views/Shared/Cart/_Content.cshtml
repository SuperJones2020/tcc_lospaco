@using TCC_LOSPACO.DAO
@using TCC_LOSPACO.Security
@using System.Threading.Tasks;
@{

    if (Authentication.IsSigned()) {

        var list = CartDAO.GetList() ;
        foreach (dynamic item in list) {
            string name = Global.GetValue(item, "Name");
            string type = Global.GetValue(item, "Type");
            if (type == "SERVICO") { @Html.Partial("Cart/_Service", CartServiceDAO.GetByName((string)name))}// else if (type == "PACOTE") { @Html.Partial("Cart/_Package", PackageDAO.GetByName(name));
    }
}
}
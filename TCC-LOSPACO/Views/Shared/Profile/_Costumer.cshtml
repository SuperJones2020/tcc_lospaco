@model TCC_LOSPACO.Models.Account
@{ 
    string value = "";
    if (!TCC_LOSPACO.DAO.AccountDAO.RegistrationCompleted(Model.Id)) { value = "main-bg-red"; }
}
<div class="data-item vertical-align-top" data-id="@Model.Id" @value data-table-of="Customer">
    <div data-zoom="1.02, .5s"
         class="data-content cursor-pointer row center-elements bg-white max-size @value">
        <div class="row col-11 col-12-child m-y-2-child">
            <div class="f-2 value email">@Model.Email</div>
        </div>
    </div>
</div>


@{
    var user = TCC_LOSPACO.Security.Authentication.GetUser();
}
<div class="item">
    <div class="nav-height"></div>
    <div class="row center-elements" style="height: calc(100% - 5rem);">
        <button class="close bg-transparent position-absolute">
            <i class="f-5 rotate-anim cursor-pointer fas fa-times z-20"></i>
        </button>
        <button type="button" data-item-button data-fill-to="down"
                class="open  bg-transparent cursor-pointer position-absolute">
            <i class="f-10 fas fa-info"><br /><span>Meus Dados</span></i>
        </button>

        <div class="row center-elements content col-10 l-col-12 max-h p-1">
            <div class="row col-11 l-col-12 max-h content-container">
                <div data-content="user" class="max-w row col-12-child align-items-center">
                    <div class="form">
                        <h1 class="f-8 text-align-center profile-title">Dados</h1>
                        <h2 class="f-4 label-title">Email atual</h2>
                        <div class="row m-t-3 customized-input max-w no-wrap">
                            <div class="col-2 row icon-input"><i class="fas fa-envelope"></i></div>
                            <input disabled class="col-10 edit-email-input" data-id="email" type="email"
                                   value=@user.Account.Email />
                            <!--<button data-anim-to-black
                                    class="col-1 cursor-pointer edit-btn row justify-content-center">
                                <i class="fas fa-pen"></i>
                            </button>
                            <button data-anim-to-black
                                    class="col-1 cursor-pointer save-btn row justify-content-center">
                                <i class="fas fa-check"></i>
                            </button>-->
                        </div>
                        <h2 class="f-4 m-t-6 label-title">Nome Completo</h2>
                        <div class="row m-t-3 customized-input max-w no-wrap">
                            <div class="col-1 row icon-input"><i class="fas fa-file-contract"></i></div>
                            <input disabled class="col-9" type="text" data-id="custfullname" value="@user.FullName" />
                            <button data-anim-to-black class="col-1 cursor-pointer edit-btn row">
                                <i class="fas fa-pen"></i>
                            </button>
                            <button data-anim-to-black class="col-1 cursor-pointer save-btn row">
                                <i class="fas fa-check"></i>
                            </button>
                        </div>
                        <h2 class="f-4 m-t-6 label-title">CPF</h2>
                        <div class="row m-t-3 customized-input max-w no-wrap">
                            <div class="col-1 row icon-input"><i class="fas fa-id-card"></i></div>
                            <input disabled class="col-9" type="number" data-id="custcpf" value="@user.CPF" />
                            <button data-anim-to-black class="col-1 cursor-pointer edit-btn row">
                                <i class="fas fa-pen"></i>
                            </button>
                            <button data-anim-to-black class="col-1 cursor-pointer save-btn row">
                                <i class="fas fa-check"></i>
                            </button>
                        </div>
                        <h2 class="f-4 m-t-6 label-title">Número</h2>
                        <div class="row m-t-3 customized-input max-w no-wrap">
                            <div class="col-1 row icon-input"><i class="fas fa-id-card"></i></div>
                            <input disabled class="col-9" type="text" data-id="custphonenumber" value="@user.Phone" />
                            <button data-anim-to-black class="col-1 cursor-pointer edit-btn row">
                                <i class="fas fa-pen"></i>
                            </button>
                            <button data-anim-to-black class="col-1 cursor-pointer save-btn row">
                                <i class="fas fa-check"></i>
                            </button>
                        </div>
                        <h2 class="f-4 m-t-6 label-title">Nome de Exibição</h2>
                        <div class="row m-t-3 customized-input max-w no-wrap">
                            <div class="col-1 row icon-input"><i class="fas fa-file-contract"></i></div>
                            <input disabled class="col-9" type="text" data-id="custusername" value="@user.Username" />
                            <button data-anim-to-black class="col-1 cursor-pointer edit-btn row">
                                <i class="fas fa-pen"></i>
                            </button>
                            <button data-anim-to-black class="col-1 cursor-pointer save-btn row">
                                <i class="fas fa-check"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>